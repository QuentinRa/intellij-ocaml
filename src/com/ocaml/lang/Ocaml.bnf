{
    parserClass="com.ocaml.lang.ocaml.OclParser"
    parserUtilClass='com.ocaml.lang.ocaml.OclParserUtil'

    // every class generated implement/extends these
    implements="com.ocaml.lang.core.OclElement"
    extends="com.ocaml.lang.core.OclElementImpl"

    // generate the types of the element here
    elementTypeHolderClass='com.ocaml.lang.core.psi.OclTypes'
    // element
    elementTypeClass="com.ocaml.lang.core.OcElementType"
    // token
    tokenTypeClass="com.ocaml.lang.core.OclTokenType"

    // prefix / suffix / output
    psiClassPrefix="Ocl"
    psiImplClassSuffix="Impl"
    psiPackage="com.ocaml.lang.core.psi"
    psiImplPackage="com.ocaml.lang.core.psi.impl"

    tokens = [
        // todo: annotations? records? double quotes?

        AND              =  'and'
        AS               =  'as'
        ASSERT           =  'assert'
        BEGIN            =  'begin'
        CLASS            =  'class'
        CONSTRAINT       =  'constraint'
